       - rpy: Z축 180° 뒤집기 (필요 없으면 0 0 0)
       - xyz: TCP 로부터의 오프셋 (예시: tool0가 Z축으로 0.16m 앞에 있음)
    -->
    <origin rpy="0 0 3.1416" xyz="0 0 0.16"/>
  </joint>

<!-- 2) Base-link to “base” for any driver TF alignment -->
  <link name="base"/>
  <joint name="link0-base_fixed_joint" type="fixed">
    <parent link="link0"/>  <!-- Indy7의 실제 root link -->
    <child  link="base"/>
    <!--
      - rpy="0 0 -π" : 필요에 따라 ±π 회전 보정
      - xyz="0 0 0"
    -->
    <origin rpy="0 0 -3.14159265" xyz="0 0 0"/>
  </joint>

<!-- 3) “world” frame → link0 연결 (시뮬레이터/MoveIt! 호환) -->
<link name="world"/>
<joint name="world_joint" type="fixed">
  <parent link="world"/>
  <child  link="link0"/>
  <!--
    - rpy, xyz 모두 0 으로 두면 world와 link0 축이 그대로 일치
    - 필요 시 rpy="0 0 -1.57" 같은 회전값을 넣어 보정
  -->
  <origin rpy="0 0 0" xyz="0 0 0"/>
</joint>

  <!-- 저 밑의 rpy 등도 걍 ur5e_robotiq85_old를 갖다 붙인건데, 이거 정확히 어떻게 설정해야하는지를 모르겠음 -->
  <!-- ===========================-->
  <!-- 3) Arm ↔ Gripper 연결부 -->
  <!-- ===========================-->
  <joint name="indy7_to_robotiq" type="fixed">
    <parent link="tcp"/>                      <!-- Indy의 TCP -->
    <child  link="robotiq_arg2f_base_link"/>  <!-- Gripper 베이스 -->
    <origin rpy="0 -1.5708 -1.5708" xyz="0 0 0"/>
  </joint>


  <!-- ================================================== -->
  <!-- eye-in-hand camera (Realsense)                     -->
  <!-- ================================================== -->
  <!-- 1) camera base frame -->
  <joint name="realsense_hand_joint" type="fixed">
    <parent link="grasp_convenient_link"/>
    <child  link="camera_hand_color_frame"/>
    <origin rpy="0 0 0"    xyz="-0.032 -0.064 -0.15"/>
  </joint>

  <!-- 2) actual camera body -->
  <link name="camera_hand_link">
    <visual>
      <origin rpy="1.5708 0 1.5708" xyz="0.00987 -0.02 -0.02"/>
      <geometry>
        <mesh filename="realsense2_description/meshes/d415.stl"/>
      </geometry>
      <material name="aluminum">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.02 0"/>
      <geometry>
        <box size="0.02005 0.099 0.023"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.564"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.003881243" ixy="0.0" ixz="0.0"
               iyy="0.00049894" iyz="0.0" izz="0.003879257"/>
    </inertial>
  </link>

  <!-- 3) camera_color frame (optical) -->
  <joint name="camera_hand_color_joint" type="fixed">
    <parent link="camera_hand_color_frame"/>
    <child  link="camera_hand_link"/>
    <origin rpy="0 -1.5708 1.5708" xyz="0 0 0"/>
  </joint>
  <link name="camera_hand_color_frame"/>

  <!-- 4) camera color → color_optical -->
  <joint name="camera_hand_color_optical_joint" type="fixed">
    <parent link="camera_hand_color_frame"/>
    <child  link="camera_hand_color_optical_frame"/>
    <origin rpy="-1.571 0 -1.571" xyz="0 0 0"/>
  </joint>
  <link name="camera_hand_color_optical_frame"/>

  <!-- 5) camera color → depth_optical (for depth) -->
  <joint name="camera_hand_depth_optical_joint" type="fixed">
    <parent link="camera_hand_color_frame"/>
    <child  link="camera_hand_depth_optical_frame"/>
    <origin rpy="-1.573 0.003 -1.576" xyz="-0.000 -0.015 -0.000"/>
  </joint>
  <link name="camera_hand_depth_optical_frame"/>





